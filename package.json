  "scripts": {
    "serve:server:prod": "env-cmd -f .env.production nx serve core --prod",
    "serve:server:dev": "env-cmd -f .env.local nx serve core",
    "serve:front:dev": "env-cmd -f packages/frontend/.env.front.dev nx serve frontend",
    "serve:front:prod": "env-cmd -f packages/frontend/.env.front.prod nx serve frontend --prod",
    "serve:batch-app:prod": "env-cmd -f .env.production nx serve batch-app --prod",
    "serve:batch-app:dev": "env-cmd -f .env.local nx serve batch-app",
    "serve:batch:prod": "env-cmd -f .env.production nx serve batch --prod",
    "serve:batch:dev": "env-cmd -f .env.local nx serve batch",
    "build:front": "env-cmd -f .env.production nx build frontend --prod",
    "build:server": "nx build core --prod",
    "build:batch": "nx build batch --prod",
    "build:batch-app": "nx build batch-app --prod",
    "start:server": "env-cmd -f .env.production pm2 start dist/packages/core/main.js --name=server",
    "start:batch": "env-cmd -f .env.production pm2 start dist/packages/batch/main.js --name=batch",
    "start:front": "pm2 start npm --name=frontend -- run serve:front:prod",
    "start:batch-app": "env-cmd -f .env.production node dist/packages/batch-app/main.js"
  },
  "private": true,
